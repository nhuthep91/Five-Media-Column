<!-- Latest compiled and minified JavaScript -->
<script src="/fiveMedia/bootstrap/js/bootstrap.js"></script>

<script>
$(document).ready(function(){
// progress bar on top 
 var lodedSize = 0;
 var number_of_media = $("body img").length;
 
	doProgress();
 
// function for the progress bar
 	function doProgress() {
		 $("img").load(function() {
 			lodedSize++;
 			var newWidthPercentage = (lodedSize / number_of_media) * 100;
 			animateLoader(newWidthPercentage + '%');
 		})
 	}
 
//Animate the loader
 	function animateLoader(newWidth) {
 		$("#progressBar").width(newWidth);
			 if(lodedSize>=number_of_media){
 				setTimeout(function(){
					 $("#progressBar").animate({opacity:0});
 				},500);
 			}
 	}

//Show up button when scroll down
 	$(window).scroll(function() {
 		if($(window).scrollTop() >= 110) {
			$(".backToTop").fadeIn();	
			}
 		else 
			$(".backToTop").fadeOut(); 
 		});
 	
 	$('.backToTop').click(function() {
 		$('html, body').animate({scrollTop:0},300);
 	});

//Show and hide search form
	$("#search_image").click(function () {
		$("#search_form").animate({
   		 width:'toggle'
   	 },"fast"); 	
	});
});

$("#black_horizontal").attributes("width","window.innerWidth");

</script>
<link rel="stylesheet" type="text/css" href="/fiveMedia/home.css">

